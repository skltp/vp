               http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-current.xsd" >

    <!-- Name of this file is mandatory and may not be changed. -->

	<flow name="clinicalprocess-healthcond-actoutcome-GetLaboratoryOrderOutcome-3.0-Interaction-virtualisering-flow-###DYNCOUNTER###" >
		<composite-source>
      	  	<https:inbound-endpoint
        	    address="https://${TP_HOST}:${TP_PORT}/${TP_BASE_URI}/clinicalprocess/healthcond/actoutcome/GetLaboratoryOrderOutcome/3/rivtabp21/###DYNCOUNTER###"
           		connector-ref="VPProducerConnector"
            	exchange-pattern="request-response"
            	responseTransformer-refs="featureKeepAliveResponse wsdlQueryResponse filterResponseHeaders">
            	<properties>
        			<spring:entry key="featureUseKeepAlive" value="${feature.keepalive.urn.riv.clinicalprocess.healthcond.actoutcome.GetLaboratoryOrderOutcomeResponder.3:${feature.keepalive}}"/>
              		<spring:entry key="featureResponseTimeout" value="${feature.featureresponsetimeout.urn.riv.clinicalprocess.healthcond.actoutcome.GetLaboratoryOrderOutcomeResponder.3:${SERVICE_TIMEOUT_MS}}"/>
        		</properties>
        	</https:inbound-endpoint>
        	<http:inbound-endpoint
        	    address="http://${TP_HOST}:${TP_PORT_HTTP}/${TP_BASE_URI}/clinicalprocess/healthcond/actoutcome/GetLaboratoryOrderOutcome/3/rivtabp21/###DYNCOUNTER###"
            	exchange-pattern="request-response"
            	responseTransformer-refs="wsdlQueryResponse filterResponseHeaders">
              <properties>
              <spring:entry key="featureResponseTimeout" value="${feature.featureresponsetimeout.urn.riv.clinicalprocess.healthcond.actoutcome.GetLaboratoryOrderOutcomeResponder.3:${SERVICE_TIMEOUT_MS}}"/>
            </properties>
        	</http:inbound-endpoint>
         </composite-source>

         <cxf:proxy-service
            	namespace="urn:riv:clinicalprocess:healthcond:actoutcome:GetLaboratoryOrderOutcome:3:rivtabp21"
            	payload="envelope"
            	service="GetLaboratoryOrderOutcomeResponderService"
            	wsdlLocation="classpath:/schemas/interactions/GetLaboratoryOrderOutcomeInteraction/GetLaboratoryOrderOutcomeInteraction_3.0_RIVTABP21.wsdl" />

